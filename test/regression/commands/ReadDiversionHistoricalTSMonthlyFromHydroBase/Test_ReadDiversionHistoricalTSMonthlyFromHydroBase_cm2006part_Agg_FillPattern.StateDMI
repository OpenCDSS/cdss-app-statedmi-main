# Test reading diversion historical TS monthly data from HydroBase for one aggregate structure
# Use the cm2006 structure
# Do not use diversion comments
# Fill parts with pattern
StartLog(LogFile="Results/Test_ReadDiversionHistoricalTSMonthlyFromHydroBase_cm2006part_Agg_FillPattern.StateDMI.log")
RemoveFile(InputFile="Results\Test_ReadDiversionHistoricalTSMonthlyFromHydroBase_cm2006part_Agg_FillPattern_out.ddh",IfNotFound=Ignore)
SetOutputPeriod(OutputStart="1949-10",OutputEnd="2004-09")
SetOutputYearType(OutputYearType=Water)
####################################################
#
SetDiversionStation(ID="39_ADC041",Name="Elk Creek",IfNotFound=Add)
SetDiversionAggregate(ID="39_ADC041",PartIDs="390500,390501,390503,390504,390510,390525,390530,390531,390536,390542,390556,390579,390592,390593,390598,390603,390615,390626,390633,390654,390655,390656,390657,390660,390661,390662,390667,390673,390674,390676,390679,390688,390689,390810,390852,390863,390934,390983,393507")
####################################################
ReadPatternFile(InputFile="Data\fill2005.pat")
ReadDiversionHistoricalTSMonthlyFromHydroBase(ID="*",UseDiversionComments=False,PatternID="09365500",FillPatternOrder=1)
# Uncomment the following command to regenerate the expected results.
# WriteDiversionHistoricalTSMonthlyToStateMod(OutputFile="ExpectedResults/Test_ReadDiversionHistoricalTSMonthlyFromHydroBase_cm2006part_Agg_FillPattern_out.ddh")
WriteDiversionHistoricalTSMonthlyToStateMod(OutputFile="Results/Test_ReadDiversionHistoricalTSMonthlyFromHydroBase_cm2006part_Agg_FillPattern_out.ddh")
CompareFiles(InputFile1="ExpectedResults/Test_ReadDiversionHistoricalTSMonthlyFromHydroBase_cm2006part_Agg_FillPattern_out.ddh",InputFile2="Results/Test_ReadDiversionHistoricalTSMonthlyFromHydroBase_cm2006part_Agg_FillPattern_out.ddh",IfDifferent=Warn)
