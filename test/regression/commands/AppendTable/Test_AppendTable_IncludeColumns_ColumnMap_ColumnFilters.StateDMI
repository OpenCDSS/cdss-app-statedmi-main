# Test appending one table to another
# * All default parameters
# * Include one column, where name does not match so use the column map
StartLog(LogFile="Results/Test_AppendTable_IncludeColumns_ColumnMap_ColumnFilters.TSTool.log")
RemoveFile(InputFile="Results/Test_AppendTable_IncludeColumns_ColumnMap_ColumnFilters_out.csv",IfNotFound=Ignore)
ReadTableFromDelimitedFile(TableID="Table1",InputFile="Data\testtable.csv")
ReadTableFromDelimitedFile(TableID="Table2",InputFile="Data\testtable2.csv")
AppendTable(TableID="Table1",AppendTableID="Table2",IncludeColumns="String2",ColumnMap="String2:String",ColumnFilters="String2:F*")
# Uncomment the following to generate the expected results.
# WriteTableToDelimitedFile(TableID="Table1",OutputFile="ExpectedResults\Test_AppendTable_IncludeColumns_ColumnMap_ColumnFilters_out.csv")
WriteTableToDelimitedFile(TableID="Table1",OutputFile="Results\Test_AppendTable_IncludeColumns_ColumnMap_ColumnFilters_out.csv")
# Do regression comparison with the original input
CompareFiles(InputFile1="ExpectedResults\Test_AppendTable_IncludeColumns_ColumnMap_ColumnFilters_out.csv",InputFile2="Results/Test_AppendTable_IncludeColumns_ColumnMap_ColumnFilters_out.csv",IfDifferent=Warn)
