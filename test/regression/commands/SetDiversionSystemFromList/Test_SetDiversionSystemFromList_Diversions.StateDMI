CompareFiles(InputFile1="ExpectedResults/Test_SetDiversionSystemFromList_Diversions_out_Collections.csv",InputFile2="Results/Test_SetDiversionSystemFromList_Diversions_out_Collections.csv",WarnIfDifferent=True)
