# Test translating crop pattern time series data using a list file
StartLog(LogFile="Results/Test_TranslateCropPatternTS_ListFile.StateDMI.log")
RemoveFile(InputFile="Results\Test_TranslateCropPatternTS_ListFile_out.cds")
SetCULocation(ID="2000505",IfNotFound=Add)
SetCULocation(ID="2000623",IfNotFound=Add)
SetCULocation(ID="2000812",IfNotFound=Add)
SetOutputPeriod(OutputStart="1998",OutputEnd="2002")
CreateCropPatternTSForCULocations(ID="*",Units="ACRE")
SetCropPatternTS(ID="2000505",SetStart=1998,SetEnd=1998,CropPattern="ALFALFA,101,CORN,102",IrrigationMethod=Flood,SupplyType=Ground)
SetCropPatternTS(ID="2000623",SetStart=1998,SetEnd=2000,CropPattern="ALFALFA,201,CORN,202",IrrigationMethod=Sprinkler,SupplyType=Surface)
SetCropPatternTS(ID="2000812",SetStart=1998,SetEnd=2002,CropPattern="ALFALFA,301,CORN,302",IrrigationMethod=Sprinkler,SupplyType=Ground)
TranslateCropPatternTS(ListFile="Data\locations.csv",IDCol=1,OldCropType="ALFALFA",NewCropType="ALFALFA2")
# Uncomment the following command to regenerate the expected results.
# WriteCropPatternTSToStateCU(OutputFile="ExpectedResults/Test_TranslateCropPatternTS_ListFile_out.cds")
WriteCropPatternTSToStateCU(OutputFile="Results/Test_TranslateCropPatternTS_ListFile_out.cds")
CompareFiles(InputFile1="ExpectedResults/Test_TranslateCropPatternTS_ListFile_out.cds",InputFile2="Results/Test_TranslateCropPatternTS_ListFile_out.cds",WarnIfDifferent=True)
