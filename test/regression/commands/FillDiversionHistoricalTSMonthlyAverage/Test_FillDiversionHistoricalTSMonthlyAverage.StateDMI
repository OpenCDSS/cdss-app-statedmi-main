# Test filling diversion historical monthly TS with average
StartLog(LogFile="Results/Test_FillDiversionHistoricalTSMonthlyAverage.StateDMI.log")
RemoveFile(InputFile="Results\Test_FillDiversionHistoricalTSMonthlyAverage_out.ddh",IfNotFound=Ignore)
# Generate data with missing on the start and end of the period.
SetOutputPeriod(OutputStart="1950-01",OutputEnd="1959-12")
SetDiversionHistoricalTSMonthlyConstant(ID="2000505",Constant=1000,SetStart="1953-01",SetEnd="1954-12",IfNotFound=Add)
SetDiversionHistoricalTSMonthlyConstant(ID="2000505",Constant=1100,SetStart="1955-01",SetEnd="1956-12")
# The following will not currently cause original limits to be recalculated.
SetDiversionHistoricalTSMonthlyConstant(ID="2000812",Constant=2000,SetStart="1953-01",SetEnd="1954-12",IfNotFound=Add)
SetDiversionHistoricalTSMonthlyConstant(ID="2000812",Constant=2100,SetStart="1955-01",SetEnd="1956-12",IfNotFound=Add)
# Fill the start and end of the period
FillDiversionHistoricalTSMonthlyAverage(ID="20005*",FillFlag=A)
# Uncomment the following command to regenerate the expected results.
# WriteDiversionHistoricalTSMonthlyToStateMod(OutputFile="ExpectedResults/Test_FillDiversionHistoricalTSMonthlyAverage_out.ddh")
WriteDiversionHistoricalTSMonthlyToStateMod(OutputFile="Results/Test_FillDiversionHistoricalTSMonthlyAverage_out.ddh")
CompareFiles(InputFile1="ExpectedResults/Test_FillDiversionHistoricalTSMonthlyAverage_out.ddh",InputFile2="Results/Test_FillDiversionHistoricalTSMonthlyAverage_out.ddh",IfDifferent=Warn)
