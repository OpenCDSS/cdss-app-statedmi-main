# Test setting diversion demand monthly TS to StateMod file (add and then overright)
StartLog(LogFile="Results/Test_SetDiversionDemandTSMonthly_StateMod.StateDMI.log")
RemoveFile(InputFile="Results\Test_SetDiversionDemandTSMonthly_StateMod_out.ddm",IfNotFound=Ignore)
SetOutputPeriod(OutputStart="2003-01",OutputEnd="2005-12")
