SetDiversionDemandTSMonthly(ID="2000505",TSID="2000505..DivTotal.Month~StateMod~Data/replace-cy.stm")
# Uncomment the following command to regenerate the expected results.
# WriteDiversionDemandTSMonthlyToStateMod(OutputFile="ExpectedResults/Test_SetDiversionDemandTSMonthly_StateMod_out.ddm")
WriteDiversionDemandTSMonthlyToStateMod(OutputFile="Results/Test_SetDiversionDemandTSMonthly_StateMod_out.ddm")
CompareFiles(InputFile1="ExpectedResults/Test_SetDiversionDemandTSMonthly_StateMod_out.ddm",InputFile2="Results/Test_SetDiversionDemandTSMonthly_StateMod_out.ddm",IfDifferent=Warn)
