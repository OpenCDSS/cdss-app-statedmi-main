WriteControlToStateMod(OutputFile="Results/Test_ReadControlFromStateMod_Sp2008L_out.ctl")
CompareFiles(InputFile1="Data/Sp2008L.ctl",InputFile2="Results/Test_ReadControlFromStateMod_Sp2008L_out.ctl",IgnoreWhitespace=True,IfDifferent=Warn)
