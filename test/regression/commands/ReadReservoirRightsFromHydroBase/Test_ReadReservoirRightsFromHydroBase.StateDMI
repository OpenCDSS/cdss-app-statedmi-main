# Test reading instream flow rights data from HydroBase for a couple of generated stations
StartLog(LogFile="Results/Test_ReadReservoirRightsFromHydroBase.StateDMI.log")
RemoveFile(InputFile="Results\Test_ReadReservoirRightsFromHydroBase_out.csv")
SetReservoirStation(ID="363543",Name="Reservoir 1",IfNotFound=Add)
SetReservoirStation(ID="364512",Name="Reservoir 2",IfNotFound=Add)
ReadReservoirRightsFromHydroBase(ID="*")
# Uncomment the following command to regenerate the expected results.
# WriteReservoirRightsToList(OutputFile="ExpectedResults/Test_ReadReservoirRightsFromHydroBase_out.csv")
WriteReservoirRightsToList(OutputFile="Results/Test_ReadReservoirRightsFromHydroBase_out.csv")
CompareFiles(InputFile1="ExpectedResults/Test_ReadReservoirRightsFromHydroBase_out.csv",InputFile2="Results/Test_ReadReservoirRightsFromHydroBase_out.csv",IfDifferent=Warn)
