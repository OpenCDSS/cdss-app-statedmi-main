WriteDiversionHistoricalTSMonthlyToStateMod(OutputFile="Results/Test_SetDiversionHistoricalTSMonthly_StateMod_out.ddh")
CompareFiles(InputFile1="ExpectedResults/Test_SetDiversionHistoricalTSMonthly_StateMod_out.ddh",InputFile2="Results/Test_SetDiversionHistoricalTSMonthly_StateMod_out.ddh",IfDifferent=Warn)
