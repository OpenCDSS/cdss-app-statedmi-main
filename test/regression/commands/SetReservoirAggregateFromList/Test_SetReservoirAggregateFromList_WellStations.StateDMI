WriteReservoirStationsToList(OutputFile="Results/Test_SetReservoirAggregateFromList_out.csv")
CompareFiles(InputFile1="ExpectedResults/Test_SetReservoirAggregateFromList_out_Collections.csv",InputFile2="Results/Test_SetReservoirAggregateFromList_out_Collections.csv",WarnIfDifferent=True)
