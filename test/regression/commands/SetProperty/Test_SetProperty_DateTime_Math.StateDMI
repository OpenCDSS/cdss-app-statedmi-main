# Test setting a processor property to an DateTime using math parameters
StartLog(LogFile="Results/Test_SetProperty_DateTime_Math.TSTool.log")
RemoveFile(InputFile="Results/Test_SetProperty_DateTime_Math_out.txt",IfNotFound=Ignore)
SetProperty(PropertyName="DateTimeProp1",PropertyType=DateTime,PropertyValue="2017-01-01T01:01:01")
SetProperty(PropertyName="DateTimeProp1",PropertyType=DateTime,PropertyValue="${DateTimeProp1}",Add=1Year)
SetProperty(PropertyName="DateTimeProp2",PropertyType=DateTime,PropertyValue="2017-01-01T01:01:01")
SetProperty(PropertyName="DateTimeProp2",PropertyType=DateTime,PropertyValue="${DateTimeProp2}",Subtract=1Year)
SetProperty(PropertyName="DateTimeProp3",PropertyType=DateTime,PropertyValue="2017-01-01T01:01:01")
SetProperty(PropertyName="DateTimeProp3",PropertyType=DateTime,PropertyValue="${DateTimeProp3}",Add=1Month)
SetProperty(PropertyName="DateTimeProp4",PropertyType=DateTime,PropertyValue="2017-01-01T01:01:01")
SetProperty(PropertyName="DateTimeProp4",PropertyType=DateTime,PropertyValue="${DateTimeProp4}",Subtract=1Month)
SetProperty(PropertyName="DateTimeProp5",PropertyType=DateTime,PropertyValue="2017-01-01T01:01:01")
SetProperty(PropertyName="DateTimeProp5",PropertyType=DateTime,PropertyValue="${DateTimeProp5}",Add=1Day)
SetProperty(PropertyName="DateTimeProp6",PropertyType=DateTime,PropertyValue="2017-01-01T01:01:01")
SetProperty(PropertyName="DateTimeProp6",PropertyType=DateTime,PropertyValue="${DateTimeProp6}",Subtract=1Day)
SetProperty(PropertyName="DateTimeProp7",PropertyType=DateTime,PropertyValue="2017-01-01T01:01:01")
SetProperty(PropertyName="DateTimeProp7",PropertyType=DateTime,PropertyValue="${DateTimeProp7}",Add=1Hour)
SetProperty(PropertyName="DateTimeProp8",PropertyType=DateTime,PropertyValue="2017-01-01T01:01:01")
SetProperty(PropertyName="DateTimeProp8",PropertyType=DateTime,PropertyValue="${DateTimeProp8}",Subtract=1Hour)
SetProperty(PropertyName="DateTimeProp9",PropertyType=DateTime,PropertyValue="2017-01-01T01:01:01")
SetProperty(PropertyName="DateTimeProp9",PropertyType=DateTime,PropertyValue="${DateTimeProp9}",Add=1Minute)
SetProperty(PropertyName="DateTimeProp10",PropertyType=DateTime,PropertyValue="2017-01-01T01:01:01")
SetProperty(PropertyName="DateTimeProp10",PropertyType=DateTime,PropertyValue="${DateTimeProp10}",Subtract=1Minute)
SetProperty(PropertyName="DateTimeProp11",PropertyType=DateTime,PropertyValue="2017-01-01T01:01:01")
SetProperty(PropertyName="DateTimeProp11",PropertyType=DateTime,PropertyValue="${DateTimeProp11}",Add=1Second)
SetProperty(PropertyName="DateTimeProp12",PropertyType=DateTime,PropertyValue="2017-01-01T01:01:01")
SetProperty(PropertyName="DateTimeProp12",PropertyType=DateTime,PropertyValue="${DateTimeProp12}",Subtract=1Second)
