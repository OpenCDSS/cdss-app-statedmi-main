# WritePropertiesToFile(OutputFile="ExpectedResults/Test_SetProperty_DateTime_Math_out.txt",IncludeProperties="DateTimeProp1,DateTimeProp2,DateTimeProp3,DateTimeProp4,DateTimeProp5,DateTimeProp6,DateTimeProp7,DateTimeProp8,DateTimeProp9,DateTimeProp10,DateTimeProp11,DateTimeProp12",FileFormat=NameValue)
# Generate new output...
WritePropertiesToFile(OutputFile="Results/Test_SetProperty_DateTime_Math_out.txt",IncludeProperties="DateTimeProp1,DateTimeProp2,DateTimeProp3,DateTimeProp4,DateTimeProp5,DateTimeProp6,DateTimeProp7,DateTimeProp8,DateTimeProp9,DateTimeProp10,DateTimeProp11,DateTimeProp12",FileFormat=NameValue)
# Do regression comparison...
CompareFiles(InputFile1="ExpectedResults\Test_SetProperty_DateTime_Math_out.txt",InputFile2="Results/Test_SetProperty_DateTime_Math_out.txt",IfDifferent=Warn)
