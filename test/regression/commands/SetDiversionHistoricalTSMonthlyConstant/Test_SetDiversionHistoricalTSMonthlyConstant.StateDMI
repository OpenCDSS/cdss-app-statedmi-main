# Test setting diversion historical monthly TS to constant (add and then overright)
StartLog(LogFile="Results/Test_SetDiversionHistoricalTSMonthlyConstant.StateDMI.log")
RemoveFile(InputFile="Results\Test_SetDiversionHistoricalTSMonthlyConstant_out.ddh")
SetOutputPeriod(OutputStart="1950-01",OutputEnd="1955-12")
SetDiversionHistoricalTSMonthlyConstant(ID="2000505",Constant=1111,IfNotFound=Add)
SetDiversionHistoricalTSMonthlyConstant(ID="2000812",Constant=2222,IfNotFound=Add)
SetDiversionHistoricalTSMonthlyConstant(ID="2000812",Constant=3333,SetStart="1950-09",SetEnd="1953-05")
# Uncomment the following command to regenerate the expected results.
# WriteDiversionHistoricalTSMonthlyToStateMod(OutputFile="ExpectedResults/Test_SetDiversionHistoricalTSMonthlyConstant_out.ddh")
WriteDiversionHistoricalTSMonthlyToStateMod(OutputFile="Results/Test_SetDiversionHistoricalTSMonthlyConstant_out.ddh")
CompareFiles(InputFile1="ExpectedResults/Test_SetDiversionHistoricalTSMonthlyConstant_out.ddh",InputFile2="Results/Test_SetDiversionHistoricalTSMonthlyConstant_out.ddh",WarnIfDifferent=True)
