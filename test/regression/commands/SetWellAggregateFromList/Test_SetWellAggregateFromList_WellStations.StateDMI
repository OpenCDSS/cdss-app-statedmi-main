# Test setting well aggregate from a list file for well stations
StartLog(LogFile="Results/Test_SetWellAggregateFromList_WellStations.StateDMI.log")
RemoveFile(InputFile="Results\Test_SetWellAggregateFromList_WellStations_out_Collections.csv")
SetWellStation(ID="Location1",Name="Well 1",IfNotFound=Add)
SetWellStation(ID="Location2",Name="Well 2",IfNotFound=Add)
SetWellStation(ID="Location3",Name="Well 3",IfNotFound=Add)
SetWellAggregateFromList(ListFile="Data\aggregates.csv",Year=1998,Div=3,PartType=Parcel,IDCol=1,NameCol=2,PartIDsCol=3,PartsListedHow=InRow)
# Uncomment the following command to regenerate the expected results.
# WriteWellStationsToList(OutputFile="ExpectedResults/Test_SetWellAggregateFromList_WellStations_out.csv")
WriteWellStationsToList(OutputFile="Results/Test_SetWellAggregateFromList_WellStations_out.csv")
CompareFiles(InputFile1="ExpectedResults/Test_SetWellAggregateFromList_WellStations_out_Collections.csv",InputFile2="Results/Test_SetWellAggregateFromList_WellStations_out_Collections.csv",IfDifferent=Warn)
