# Test reading diversion rights data from HydroBase for a subset of structures
StartLog(LogFile="Results/Test_ReadDiversionRightsFromColoradHydroBaseRest.StateDMI.log")
RemoveFile(InputFile="Results\Test_ReadDiversionRightsFromColoradoHydroBaseRest_rg2007part_out.csv")
####################################################
# Start defining test stations and aggregate information...
# The selected locations represent the combinations of cases to test but are
# small enough to run quickly.  Full data sets are validated in other tests.
#
## Below are D&W structures
#
SetDiversionStation(ID="2000511",Name="ANACONDA D",IfNotFound=Add)
#
## Below are diversion aggregate structures - define div and aggregates
#
SetDiversionStation(ID="20ADW02",Name="20ADW02",IfNotFound=Add)
SetDiversionAggregate(ID="20ADW02",PartIDs="2000895,2000561,2000721,2000751,2000695")
#
## Below are diversion system structures - define div and system
#
SetDiversionStation(ID="20MS01",Name="20MS01",IfNotFound=Add)
SetDiversionSystem(ID="20MS01",PartIDs="2000613, 2001004")
#
##...end defining test stations and aggregates
####################################################
ReadDiversionRightsFromHydroBase(ID="*",Datastore=ColoradoHydroBaseRest)
# Uncomment the following command to regenerate the expected results.
# WriteDiversionRightsToList(OutputFile="ExpectedResults/Test_ReadDiversionRightsFromColoradoHydroBaseRest_rg2007part_out.csv")
WriteDiversionRightsToList(OutputFile="Results/Test_ReadDiversionRightsFromColoradoHydroBaseRest_rg2007part_out.csv")
CompareFiles(InputFile1="ExpectedResults/Test_ReadDiversionRightsFromColoradoHydroBaseRest_rg2007part_out.csv",InputFile2="Results/Test_ReadDiversionRightsFromColoradoHydroBaseRest_rg2007part_out.csv",IfDifferent=Warn)
