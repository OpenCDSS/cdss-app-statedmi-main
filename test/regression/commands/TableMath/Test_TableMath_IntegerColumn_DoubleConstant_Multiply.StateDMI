# Test multiplying table column and a constant
# - column is integers
# - constant is double
StartLog(LogFile="Results/Test_TableMath_IntegerColumn_DoubleConstant_Multiply.TSTool.log")
RemoveFile(InputFile="Results/Test_TableMath_IntegerColumn_DoubleConstant_Multiply_out.dv",IfNotFound=Ignore)
ReadTableFromDelimitedFile(TableID="Table1",InputFile="Data\testdata.csv")
# Perform math on the table
TableMath(TableID="Table1",Input1="Integer1",Operator="*",Input2="2.0",Output="result")
# Uncomment the following command to regenerate expected results.
# WriteTableToDelimitedFile(TableID="Table1",OutputFile="ExpectedResults/Test_TableMath_IntegerColumn_DoubleConstant_Multiply_out.csv")
WriteTableToDelimitedFile(TableID="Table1",OutputFile="Results/Test_TableMath_IntegerColumn_DoubleConstant_Multiply_out.csv")
CompareFiles(InputFile1="ExpectedResults/Test_TableMath_IntegerColumn_DoubleConstant_Multiply_out.csv",InputFile2="Results/Test_TableMath_IntegerColumn_DoubleConstant_Multiply_out.csv",IfDifferent=Warn)
