CompareFiles(InputFile1="ExpectedResults/Test_TableMath_IntegerColumn_DoubleConstant_Add_out.csv",InputFile2="Results/Test_TableMath_IntegerColumn_DoubleConstant_Add_out.csv",IfDifferent=Warn)
