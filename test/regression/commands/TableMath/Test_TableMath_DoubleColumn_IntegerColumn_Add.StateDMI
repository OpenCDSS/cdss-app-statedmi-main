# Test adding 2 table columns
# - first is double
# - second is integer
StartLog(LogFile="Results/Test_TableMath_DoubleColumn_IntegerColumn_Add.TSTool.log")
RemoveFile(InputFile="Results/Test_TableMath_DoubleColumn_IntegerColumn_Add_out.dv",IfNotFound=Ignore)
ReadTableFromDelimitedFile(TableID="Table1",InputFile="Data\testdata.csv")
# Perform math on the table
TableMath(TableID="Table1",Input1="Double1",Operator="+",Input2="Integer2",Output="result")
# Uncomment the following command to regenerate expected results.
# WriteTableToDelimitedFile(TableID="Table1",OutputFile="ExpectedResults/Test_TableMath_DoubleColumn_IntegerColumn_Add_out.csv")
WriteTableToDelimitedFile(TableID="Table1",OutputFile="Results/Test_TableMath_DoubleColumn_IntegerColumn_Add_out.csv")
CompareFiles(InputFile1="ExpectedResults/Test_TableMath_DoubleColumn_IntegerColumn_Add_out.csv",InputFile2="Results/Test_TableMath_DoubleColumn_IntegerColumn_Add_out.csv",IfDifferent=Warn)
