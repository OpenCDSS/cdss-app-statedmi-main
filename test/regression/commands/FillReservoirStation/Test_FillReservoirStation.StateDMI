# Test filling reservoir station data with a couple of generated stations
StartLog(LogFile="Results/Test_FillReservoirStation.StateDMI.log")
RemoveFile(InputFile="Results\Test_FillReservoirStation_out.csv")
RemoveFile(InputFile="Results\Test_FillReservoirStation_out_Accounts.csv")
RemoveFile(InputFile="Results\Test_FillReservoirStation_out_ContentAreaSeepage.csv")
RemoveFile(InputFile="Results\Test_FillReservoirStation_out_PrecipStations.csv")
RemoveFile(InputFile="Results\Test_FillReservoirStation_out_EvapStations.csv")
RemoveFile(InputFile="Results\Test_FillReservoirStation_out_Collections.csv")
SetReservoirStation(ID="Reservoir1",Name="Reservoir 1",IfNotFound=Add)
SetReservoirStation(ID="Reservoir2",Name="Reservoir 2",IfNotFound=Add)
FillReservoirStation(ID="Reservoir1*",Name="Filled res",RiverNodeID="ID",OnOff=1,OneFillRule=1,DailyID="ID",ContentMin=1,ContentMax=1111,ReleaseMax=111,DeadStorage=111,AccountID=1,AccountName="Filled account",AccountMax=100,AccountInitial=10,AccountEvap=100,AccountOneFill=1,EvapStations="evap1,80,evap2,20",PrecipStations="prec1,81,prec2,19",ContentAreaSeepage="0,0,0;1,1,1")
# Uncomment the following command to regenerate the expected results.
# WriteReservoirStationsToList(OutputFile="ExpectedResults/Test_FillReservoirStation_out.csv")
WriteReservoirStationsToList(OutputFile="Results/Test_FillReservoirStation_out.csv")
CompareFiles(InputFile1="ExpectedResults/Test_FillReservoirStation_out.csv",InputFile2="Results/Test_FillReservoirStation_out.csv",WarnIfDifferent=True)
CompareFiles(InputFile1="ExpectedResults/Test_FillReservoirStation_out_Accounts.csv",InputFile2="Results/Test_FillReservoirStation_out_Accounts.csv",WarnIfDifferent=True)
CompareFiles(InputFile1="ExpectedResults/Test_FillReservoirStation_out_ContentAreaSeepage.csv",InputFile2="Results/Test_FillReservoirStation_out_ContentAreaSeepage.csv",WarnIfDifferent=True)
CompareFiles(InputFile1="ExpectedResults/Test_FillReservoirStation_out_PrecipStations.csv",InputFile2="Results/Test_FillReservoirStation_out_PrecipStations.csv",WarnIfDifferent=True)
CompareFiles(InputFile1="ExpectedResults/Test_FillReservoirStation_out_EvapStations.csv",InputFile2="Results/Test_FillReservoirStation_out_EvapStations.csv",WarnIfDifferent=True)
CompareFiles(InputFile1="ExpectedResults/Test_FillReservoirStation_out_Collections.csv",InputFile2="Results/Test_FillReservoirStation_out_Collections.csv",WarnIfDifferent=True)
