# Test copying an entire table using a table read from a file
StartLog(LogFile="Results/Test_CopyTable_IncludeColumns.TSTool.log")
RemoveFile(InputFile="Results/Test_CopyTable_IncludeColumns_out.csv",IfNotFound=Ignore)
ReadTableFromDelimitedFile(TableID="Table1",InputFile="Data\testtable.csv")
CopyTable(TableID="Table1",NewTableID="Table1Copy",IncludeColumns="DateTime")
# Uncomment the following to generate the expected results.
# WriteTableToDelimitedFile(TableID="Table1Copy",OutputFile="ExpectedResults\Test_CopyTable_IncludeColumns_out.csv")
WriteTableToDelimitedFile(TableID="Table1Copy",OutputFile="Results\Test_CopyTable_IncludeColumns_out.csv")
# Do regression comparison with the original input
CompareFiles(InputFile1="ExpectedResults\Test_CopyTable_IncludeColumns_out.csv",InputFile2="Results/Test_CopyTable_IncludeColumns_out.csv",IfDifferent=Warn)
