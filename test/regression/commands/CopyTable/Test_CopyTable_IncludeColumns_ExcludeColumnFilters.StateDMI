# Test copying a table read from a file
# * Specify columns to include in copy
# * Specify column exclude filter
StartLog(LogFile="Results/Test_CopyTable_IncludeColumns_ColumnExcludeFilters.TSTool.log")
RemoveFile(InputFile="Results/Test_CopyTable_IncludeColumns_ColumnExcludeFilters_out.csv",IfNotFound=Ignore)
ReadTableFromDelimitedFile(TableID="Table1",InputFile="Data\testtable2.csv")
CopyTable(TableID="Table1",NewTableID="Table1Copy",IncludeColumns="DateTime,String",ColumnExcludeFilters="String:F*,String:*fth*")
# Uncomment the following to generate the expected results.
# WriteTableToDelimitedFile(TableID="Table1Copy",OutputFile="ExpectedResults\Test_CopyTable_IncludeColumns_ColumnExcludeFilters_out.csv")
WriteTableToDelimitedFile(TableID="Table1Copy",OutputFile="Results\Test_CopyTable_IncludeColumns_ColumnExcludeFilters_out.csv")
# Do regression comparison with the original input
CompareFiles(InputFile1="ExpectedResults\Test_CopyTable_IncludeColumns_ColumnExcludeFilters_out.csv",InputFile2="Results/Test_CopyTable_IncludeColumns_ColumnExcludeFilters_out.csv",IfDifferent=Warn)
