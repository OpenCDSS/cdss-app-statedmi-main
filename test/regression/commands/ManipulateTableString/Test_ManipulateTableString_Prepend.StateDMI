# Test prepending a string to a string column in a table
StartLog(LogFile="Results/Test_ManipulateTableString_Columns_Prepend.TSTool.log")
RemoveFile(InputFile="Results/Test_ManipulateTableString_Columns_Prepend_out.dv",IfNotFound=Ignore)
# Generate some table data but converting time series to table
ReadTableFromDelimitedFile(TableID="Table1",InputFile="Data\testdata.csv")
# Manipulate strings in the table
ManipulateTableString(TableID="Table1",InputColumn1="String1",Operator="Prepend",InputValue2="Prefix:",OutputColumn="String3")
# Uncomment the following command to regenerate expected results.
# WriteTableToDelimitedFile(TableID="Table1",OutputFile="ExpectedResults/Test_ManipulateTableString_Columns_Prepend_out.csv")
WriteTableToDelimitedFile(TableID="Table1",OutputFile="Results/Test_ManipulateTableString_Columns_Prepend_out.csv")
CompareFiles(InputFile1="ExpectedResults/Test_ManipulateTableString_Columns_Prepend_out.csv",InputFile2="Results/Test_ManipulateTableString_Columns_Prepend_out.csv",IfDifferent=Warn)
